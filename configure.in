			,with_freetype1includedir=) 
AC_ARG_WITH(freetype1libdir,   [  --with-freetype1libdir=DIR         Use freetype1 lib from the given directory],
			,with_freetype1libdir=) 

if test "$enable_freetype1" = no && test "$with_freetype1" = yes; then
    enable_freetype1=yes
    with_freetype1=
fi

if test "$enable_freetype1" = no && test "$with_freetype1"X != "X"; then
    enable_freetype1=yes
fi

if test "$enable_freetype1" = yes; then
	ac_gdz_freetype1_incdir=
	ac_gdz_freetype1_libdir=
	ac_gdz_conf_package_defs=$ac_gdz_conf_package_defs' -DOSG_WITH_FREETYPE1'
	ac_gdz_conf_package_freetype1='1'
	ac_gdz_conf_package_freetype1_e1='1'

    if test -n "$with_freetype1"; then
		ac_gdz_freetype1_incdir=$with_freetype1/include

        ac_gdz_freetype1_libdir=$with_freetype1/lib

        if ! test -d "$ac_gdz_freetype1_incdir" && ! test -n "$with_freetype1includedir"; then
            echo "ERROR freetype1 include dir does not seem to exist, please check the given base dir \"$with_freetype1\""
            exit
        fi
        if ! test -d "$ac_gdz_freetype1_libdir" && ! test -n "$with_freetype1libdir"; then
            echo "ERROR freetype1 lib dir does not seem to exist, please check the given base dir \"$with_freetype1\""
            exit
        fi
    fi

    if test -n "$with_freetype1includedir"; then
		ac_gdz_freetype1_incdir=$with_freetype1includedir

        if ! test -d "$ac_gdz_freetype1_incdir"; then
            echo "ERROR freetype1 include dir does not seem to exist, please check the given dir \"$with_freetype1includedir\""
            exit
        fi
    fi

    if test -n "$with_freetype1libdir"; then
	    ac_gdz_freetype1_libdir=$with_freetype1libdir

        if ! test -d "$ac_gdz_freetype1_libdir"; then
            echo "ERROR freetype1 lib dir does not seem to exist, please check the given dir \"$with_freetype1libdir\""
            exit
        fi
    fi
else
	ac_gdz_freetype1_dir=
	ac_gdz_freetype1_incdir=
	ac_gdz_freetype1_libdir=
fi

