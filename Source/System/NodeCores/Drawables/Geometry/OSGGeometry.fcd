	<Field
		name="texCoords3"
		type="GeoTexCoordsPtr"
		cardinality="single"
		visibility="external"
		access="public"
	>
	</Field>
	<Field
		name="indices"
		type="GeoIndicesPtr"
		cardinality="single"
		visibility="external"
		access="public"
