		visibility="external"
		access="public"
	>
	</Field>
	<Field
		name="positions"
		type="GeoPositionsPtr"
		cardinality="single"
		visibility="external"
		access="public"
	>
	</Field>
	<Field
		name="normals"
		type="GeoNormalsPtr"
		cardinality="single"
		visibility="external"
		access="public"
	>
	</Field>
	<Field
		name="colors"
		type="GeoColorsPtr"
		cardinality="single"
		visibility="external"
		access="public"
	>
	</Field>
	<Field
		name="secondaryColors"
		type="GeoColorsPtr"
		cardinality="single"
		visibility="external"
		access="public"
	>
	</Field>
	<Field
