	structure="concrete"
	systemcomponent="true"
	parentsystemcomponent="true"
	decoratable="false"
	useLocalIncludes="false"
>
	<Field
		name="chunks"
		type="StateChunkPtr"
		cardinality="multi"
		visibility="external"
		access="public"
	>
	</Field>
	<Field
		name="slots"
		type="Int32"
		cardinality="multi"
		visibility="external"
		access="public"
	>
	</Field>
	<Field
		name="transparencyMode"
		type="Int32"
		cardinality="single"
		visibility="external"
		defaultValue="0"
		access="public"
	>
	</Field>
</FieldContainer>
