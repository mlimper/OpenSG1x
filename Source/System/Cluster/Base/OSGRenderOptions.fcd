<?xml version="1.0"?>

<FieldContainer
	name="RenderOptions"
	parent="Attachment"
	library="System"
	pointerfieldtypes="both"
	structure="concrete"
	systemcomponent="true"
	parentsystemcomponent="true"
	decoratable="false"
	useLocalIncludes="false"
>
	<Field
		name="statistic"
		type="bool"
		cardinality="single"
		visibility="external"
		defaultValue="false"
		access="public"
	>
	</Field>
	<Field
		name="polygonMode"
		type="GLenum"
		cardinality="single"
		visibility="external"
		defaultValue="GL_FILL"
		defaultHeader="<OSGGL.h>"
		access="public"
	>
	</Field>
	<Field
		name="twoSidedLighting"
		type="bool"
		cardinality="single"
		visibility="external"
		defaultValue="false"
		access="public"
	>
	</Field>
	<Field
		name="specTexLighting"
		type="bool"
		cardinality="single"
		visibility="external"
		defaultValue="false"
		access="public"
	>
	</Field>
	<Field
		name="sortTrans"
		type="bool"
		cardinality="single"
		visibility="external"
		defaultValue="true"
		access="public"
	>
	</Field>
	<Field
		name="zWriteTrans"
		type="bool"
		cardinality="single"
		visibility="external"
		defaultValue="true"
		access="public"
	>
	</Field>
	<Field
		name="localLights"
		type="bool"
		cardinality="single"
		visibility="external"
		defaultValue="true"
		access="public"
	>
	</Field>
	<Field
		name="correctTwoSidedLighting"
		type="bool"
		cardinality="single"
		visibility="external"
		defaultValue="true"
		access="public"
	>
	</Field>
	<Field
		name="occlusionCulling"
		type="bool"
		cardinality="single"
		visibility="external"
		defaultValue="false"
		access="public"
	>
	</Field>
	<Field
		name="occlusionCullingMode"
		type="Int32"
		cardinality="single"
		visibility="external"
		defaultValue="1"
		access="public"
	>
	OcclusionStopAndWait (1) or OcclusionMultiFrame (2)
	</Field>
	<Field
		name="occlusionCullingPixels"
		type="UInt32"
		cardinality="single"
		visibility="external"
		defaultValue="0"
		access="public"
	>
	culls objects with this number of visible pixels default is zero.
	</Field>
	<Field
		name="antialiasing"
		type="bool"
		cardinality="single"
		visibility="external"
		defaultValue="false"
		access="public"
	>
	</Field>
	<Field
		name="antialiasingDistance"
		type="Real32"
		cardinality="single"
		visibility="external"
		defaultValue="0.2"
		access="public"
	>
	</Field>
	<Field
		name="antialiasingScale"
		type="Real32"
		cardinality="single"
		visibility="external"
		defaultValue="2.0"
		access="public"
	>
	</Field>
	<Field
		name="antialiasingTrigger"
		type="UInt32"
		cardinality="single"
		visibility="external"
		defaultValue="0"
		access="public"
	>
	</Field>
	<Field
		name="frustumCulling"
		type="bool"
		cardinality="single"
		visibility="external"
		defaultValue="false"
		access="public"
	>
	</Field>
	<Field
		name="backfaceCulling"
		type="bool"
		cardinality="single"
		visibility="external"
		defaultValue="false"
		access="public"
	>
	</Field>
	<Field
		name="smallFeatureCulling"
		type="bool"
		cardinality="single"
		visibility="external"
		access="public"
	>
	</Field>
	<Field
		name="smallFeaturePixels"
		type="Real32"
		cardinality="single"
		visibility="external"
		access="public"
	>
	</Field>
	<Field
		name="smallFeatureThreshold"
		type="UInt32"
		cardinality="single"
		visibility="external"
		access="public"
	>
	</Field>
	<Field
		name="firstFrame"
		type="bool"
		cardinality="single"
		visibility="external"
		defaultValue="true"
		access="public"
	>
	</Field>
</FieldContainer>
