#! /bin/sh

if ! test -w Base/tmp_gv; then 
    echo "Need Base/tmp_gv"
	echo "mkdir Base/tmp_gv"

    mkdir Base/tmp_gv
fi

if ! test -w Field/tmp_gv; then 
    echo "Need Field/tmp_gv"
	echo "mkdir Field/tmp_gv"

    mkdir Field/tmp_gv
fi

if ! test -w Loader/OBJ/tmp_gv; then 
    echo "Need Loader/OBJ/tmp_gv"
	echo "mkdir Loader/OBJ/tmp_gv"

    mkdir Loader/OBJ/tmp_gv
fi

if ! test -w Loader/WRL/tmp_gv; then 
    echo "Need Loader/WRL/tmp_gv"
	echo "mkdir Loader/WRL/tmp_gv"

