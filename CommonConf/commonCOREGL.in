
INCLUDE_COREGL := 
LINK_COREGL    := 
LIB_COREGL     := -framework OpenGL -framework Carbon

ifneq ($(INCLUDE_COREGL),)
INCL_$(OS_BASE) += $(INC_OPTION)$(INCLUDE_COREGL)
endif

ifneq ($(LINK_COREGL),)
LIBPATHS_COREGL := $(LIBPATH_OPTION)$(LINK_COREGL)
endif

ifneq ($(LIB_COREGL),)
LIB_FILE_COREGL     := $(LIB_COREGL)
LIB_FILE_COREGL_LNK := $(LIB_COREGL)
OSG_LIB_EXT         := $(OSG_LIB_EXT) $(LIB_COREGL)
endif
